# AWS Credentials (obrigatório)
AWS_ACCESS_KEY_ID=your_aws_access_key_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_key_here
AWS_DEFAULT_REGION=us-east-1

# S3 Configuration (obrigatório)
S3_BUCKET_NAME=your-backup-bucket-name
S3_BUCKET_PATH=/backups

# Garbage Collection Settings
BACKUP_RETENTION_DAYS=7
DRY_RUN=false
VERBOSE=true

# Cron Schedule (apenas para execução periódica)
# Exemplo: "0 3 * * *" = todos os dias às 3h da manhã
# Exemplo: "0 */6 * * *" = a cada 6 horas
# Exemplo: "0 2 * * 0" = todos os domingos às 2h da manhã
CRON_SCHEDULE=0 3 * * *